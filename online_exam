    function exam()
    {
    var m=0;
    if(document.examForm.one.value=="u tag")
    m=m+1;
    if(document.examForm.two.value=="7")
    m=m+1;
    if(document.examForm.three.value=="HTML")
    m=m+1;
    alert("Thankyou for taking the online exam \n Your score is:"+m+" out of 3");
    }
    </script>
